<html lang="en">
    <%- include('../partials/head.ejs') %>
    <body>

        <%- include('../partials/nav.ejs') %>

        <div class="create-blog content">
            <form action="/banks" method="POST">
                <input type="hidden" id="churchId" name="churchId" value="<%= churchId %>">
                <label for="accountName">Account Name</label>
                <input type="text" id="accountName" name="accountName" required>
                <label for="accountNumber">Account Number</lable>
                <input type="text" id="accountNumber" name="accountNumber" placeholder="xxxx1234" required>
                <label for="description">Description</label>
                <input type="text" id="description" name="description" >
                <input type="hidden" id="status" name="status"  value="Active" >
                <button>Submit</button>
            </form>
        </div>
        <%- include('../partials/footer.ejs') %>
    </body>

</html>