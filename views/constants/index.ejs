<html lang="en">
    <%- include('../partials/head.ejs') %>
    <body>
          <%- include('../partials/nav.ejs') %>
          
        <div class="blogs content">
            <h1><%= global.churchName %></h1>
            <a href="/constants/create/<%= churchId %>">Add Constant</a>
            <h2>All Constants</h2>
            <% if(constants === null) { %>
                <p>No constants data available</p>
               <% } else { %>
                <% if(constants.length > 0) { %>
                    <% constants.forEach(constant => { %>
                        <a class="single" href="/constants/<%= constant._id %>" >
                    <h3 class="title"><%= constant.category %> | <%= constant.name %> | <%= constant.value1 %> </h3>
                    </a>
                    <% }) %>
                <% } %>
           <% } %>
           
        </div>
        <%- include('../partials/footer.ejs') %>
    </body>

</html>