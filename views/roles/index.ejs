<html lang="en">
    <%- include('../partials/head.ejs') %>
    <body>
          <%- include('../partials/nav.ejs') %>
          
        <div class="blogs content">
            <a href="/roles/create">Add Role</a>
            <h2>All Roles</h2>
            <% if(roles.length > 0) { %>
                <% roles.forEach(role => { %>
                    <a class="single" href="/roles/<%= role._id %>" >
                <h3 class="title"><%= role.userId %> </h3>
                <p class="snippet"><%= role.roleId %></p>
                </a>
                <% }) %>
            <% } %>
        </div>
        <%- include('../partials/footer.ejs') %>
    </body>

</html>